<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="sj.tool.betanote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_betanote"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_betanote_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="sj.tool.betanote.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sj.tool.betanote.Bnote"
                  android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="sj.tool.betanote.About"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".GeneralSettings"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>